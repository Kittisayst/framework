# ການຕັ້ງຄ່າແອັບພລິເຄຊັນ
APP_NAME=MyFramework
APP_URL=http://localhost/framework
APP_DEBUG=true

# ການຕັ້ງຄ່າຖານຂໍ້ມູນ
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=product
DB_USERNAME=root
DB_PASSWORD=

# ການຕັ້ງຄ່າ session
SESSION_LIFETIME=120
SESSION_SECURE=false

# ການຕັ້ງຄ່າ timezone
TIMEZONE=Asia/Bangkok